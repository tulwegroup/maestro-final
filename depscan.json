{
  "$schema": "https://raw.githubusercontent.com/owasp-dep-scan/dep-scan/master/schema.json",
  "projectName": "MAESTRO",
  "projectType": "nodejs",
  "severityThreshold": "medium",
  "failOn": "critical",
  "skipProjects": [
    "examples/**",
    "skills/**"
  ],
  "ignoreVulnerabilities": [],
  "licensePolicy": {
    "allowedLicenses": [
      "MIT",
      "Apache-2.0",
      "BSD-2-Clause",
      "BSD-3-Clause",
      "ISC",
      "0BSD"
    ],
    "warnLicenses": [
      "LGPL-2.1",
      "LGPL-3.0",
      "MPL-2.0"
    ],
    "blockLicenses": [
      "GPL-2.0",
      "GPL-3.0",
      "AGPL-3.0",
      "SSPL-1.0"
    ]
  },
  "vulnerabilityThresholds": {
    "critical": 0,
    "high": 5,
    "medium": 20,
    "low": -1
  }
}
